import java.util.* ;
import java.io.*; 
public class Solution {
    
	public static int uniquePaths(int m, int n) {
		// Write your code here.
        int t=m+n-2;
        int r=n-1;
        int res=1;
        for(int i=1;i<=r;i++)
        {
            res=res*(t-r+i)/i;
        }
       return res; 
	}
    
}